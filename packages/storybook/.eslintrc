{
  "extends": ["jam"],
  "globals": {
    "window": true,
    "Promise": true
  },
  "rules": {
    "no-empty": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["^~", "^./lib", "^@reactant"]
      }
    ]
  }
}
