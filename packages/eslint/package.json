{
  "name": "eslint-config-reactant",
  "version": "0.1.141",
  "description": "reactant eslint",
  "keywords": [
    "reactant",
    "eslint"
  ],
  "homepage": "https://github.com/codejamninja/reactant/packages/eslint",
  "bugs": {
    "url": "https://github.com/codejamninja/reactant/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "base.js",
    "lib",
    "typescript"
  ],
  "main": "lib/index.js",
  "bin": {
    "reactant": "bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/reactant"
  },
  "scripts": {
    "build": "npm run test && babel src -d lib",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "format": "prettier --write ./src/**/*.js",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "test": "npm run lint:fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.3.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^6.5.1",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "base.js",
    "lib"
  ]
}
