{
  "name": "@reactant/cli",
  "version": "0.0.3",
  "description": "build tool for reactant",
  "homepage": "https://reactant.codejam.ninja",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "npm run test && npm run build:lib && npm run build:bin && npm run build:err",
    "build:bin": "mkdir -p bin && babel src/bin.js -o bin/reactant.js",
    "build:err": "parcel build src/views/error.html -d lib/views --out-file error.ejs",
    "build:lib": "babel src --ignore bin.js -d lib",
    "clean": "rimraf lib bin .cache",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "start": "babel-node src/reactant",
    "test": "npm run lint && jest --forceExit",
    "test:reset": "jest --clearCache && npm run test"
  },
  "bin": {
    "reactant": "bin/reactant.js"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
