{
  "name": "reaction-build",
  "version": "0.0.14",
  "description": "Build tool for reaction",
  "homepage": "reaction.codejam.ninja",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "npm run clean && npm run test && npm run build:lib && npm run build:bin",
    "build:bin": "mkdir bin && babel src/bin.js -o bin/reaction.js",
    "build:lib": "babel src --ignore bin.js -d lib",
    "clean": "rimraf lib bin",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run build",
    "start": "babel-node src/reaction",
    "test": "npm run lint && jest",
    "test:reset": "jest --clearCache && npm run test"
  },
  "bin": {
    "reaction": "bin/reaction.js"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-web": "^0.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "circular-json": "^0.5.3",
    "cross-environment": "0.0.8",
    "easycp": "0.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.5",
    "loglevel": "^1.6.1",
    "markdown-loader": "^2.0.2",
    "ora": "^2.1.0",
    "rc-config": "0.0.5",
    "react-art": "^16.3.2",
    "react-dev-utils": "^5.0.1",
    "react-error-overlay": "^4.0.0",
    "react-native-web": "^0.6.1",
    "sockjs-client": "^1.1.4",
    "start-server-webpack-plugin": "^2.2.5",
    "strip-ansi": "^4.0.0",
    "url": "^0.11.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.7.2",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "eslint-plugin-jest": "^21.15.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "repository": "https://github.com/codejamninja/reaction-build",
  "engines": {
    "node": ">= 4.0.0"
  },
  "eslintIgnore": [
    "lib",
    "bin"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/[^_]*.js"
    ],
    "verbose": true
  }
}
