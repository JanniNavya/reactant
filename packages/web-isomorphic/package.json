    "fs-extra": "^7.0.0",
    "hmr-client": "^0.1.3",
    "html-webpack-plugin": "^3.2.0",
    "ignore-warnings": "^0.2.0",
    "isomorphic-style-loader": "^4.0.0",
    "lodash": "^4.17.11",
    "merge-configuration": "^0.2.0",
    "newregexp": "^1.0.1",
    "node-sass": "^4.9.4",
    "offline-plugin": "^5.0.5",
    "open": "^0.0.5",
    "ora": "^3.0.0",
