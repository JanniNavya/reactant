{
  "name": "reaction-base",
  "version": "1.0.0-rc.18",
  "description": "reaction core components",
  "homepage": "https://github.com/codejamninja/reaction-base",
  "bugs": {
    "url": "https://github.com/codejamninja/reaction-base/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
