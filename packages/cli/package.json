    "detect-port": "^1.2.3",
    "easycp": "^0.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "html-loader": "^0.5.5",
