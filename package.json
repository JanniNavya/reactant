{
  "name": "reaction-build",
  "version": "0.2.26",
  "description": "Build tool for reaction",
  "homepage": "https://reaction.codejam.ninja",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "npm run clean && npm run test && npm run build:lib && npm run build:bin && npm run build:err",
    "build:bin": "mkdir bin && babel src/bin.js -o bin/reaction.js",
    "build:lib": "babel src --ignore bin.js -d lib",
    "build:err": "parcel build src/views/error.html -d lib/views --out-file error.ejs",
    "clean": "rimraf lib bin .cache",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run build",
    "start": "babel-node src/reaction",
    "test": "npm run lint && jest",
    "test:reset": "jest --clearCache && npm run test"
  },
  "bin": {
    "reaction": "bin/reaction.js"
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@storybook/react-native": "^3.4.8",
    "assets-webpack-plugin": "^3.8.4",
    "autobind-decorator": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.8.8",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "circular-json": "^0.5.5",
    "cross-environment": "0.0.8",
    "deasync": "^0.1.13",
    "detect-port": "^1.2.3",
    "easycp": "^0.1.1",
    "ejs": "^2.6.1",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "html-loader": "^0.5.5",
    "ignore-warnings": "^0.1.0",
    "markdown-loader": "^3.0.0",
    "ora": "^2.1.0",
    "rc-config": "0.0.5",
    "react-art": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-error-overlay": "^4.0.0",
    "reaction-base": "0.0.14",
    "sockjs-client": "^1.1.5",
    "start-server-webpack-plugin": "^2.2.5",
    "strip-ansi": "^4.0.0",
    "url": "^0.11.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "core-js": "^2.5.7",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.3.0",
    "native-base-web": "github:codejamninja/native-base-web#npm",
    "parcel-bundler": "^1.9.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-native-web": "^0.8.8",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "repository": "https://github.com/codejamninja/reaction-build",
  "engines": {
    "node": ">= 4.0.0"
  },
  "eslintIgnore": [
    "lib",
    "bin"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/[^_]*.js"
    ],
    "verbose": true
  }
}
