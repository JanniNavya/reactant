    "@storybook/react": "^3.4.10",
    "@storybook/react-native": "^3.4.10",
    "assets-webpack-plugin": "^3.9.6",
    "autobind-decorator": "^2.1.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-everything": "^0.1.2",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "circular-json": "^0.5.5",
    "css-loader": "^1.0.0",
    "deasync": "^0.1.13",
    "detect-port": "^1.2.3",
    "easycp": "^0.1.1",
    "ejs": "^2.6.1",
    "eslint": "^5.5.0",
