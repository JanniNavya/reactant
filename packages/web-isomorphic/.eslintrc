{
  "extends": ["jam"],
  "globals": {
    "window": true,
    "document": true
  },
  "rules": {
    "global-require": "off",
    "no-empty": "off",
    "prefer-rest-params": "off",
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["^~", "^@reactant/web-isomorphic"]
      }
    ]
  }
}
