{
  "name": "@reactant/cli",
  "version": "0.0.54",
  "description": "build tool for reactant",
  "homepage": "https://reactant.codejam.ninja",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "action.js",
    "bin",
    "config",
    "lib",
    "webpack"
  ],
  "scripts": {
    "build": "npm run test && babel src --ignore bin.js -d lib",
    "clean": "rimraf lib .cache",
    "format": "prettier --write ./src/**/*.js",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "start": "babel-node src/reactant",
    "test": "npm run lint && jest --forceExit",
    "test:reset": "jest --clearCache && npm run test"
  },
  "bin": {
    "reactant": "bin/reactant.js"
  },
  "dependencies": {
    "@reactant/core": "^0.0.54",
    "bluebird": "^3.5.2",
    "circular-json": "^0.5.7",
    "commander": "^2.19.0",
    "css-loader": "^1.0.1",
    "deasync": "^0.1.13",
    "detect-port": "^1.2.3",
    "err": "^2.1.10",
    "export-es6": "^0.1.0",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "html-loader": "^0.5.5",
    "js-info": "^0.3.0",
    "linked-deps": "^0.1.0",
    "lodash": "^4.17.11",
    "markdown-loader": "^4.0.0",
    "merge-configuration": "^0.3.2",
    "node-ipc": "^9.1.1",
    "ora": "^3.0.0",
    "pkg-dir": "^3.0.0",
    "rc-config": "^0.1.0",
    "trailduck": "^0.2.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-async-super": "^1.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.6.1",
    "eslint-config-jam": "^0.1.4",
    "eslint-plugin-jest": "^21.25.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "repository": "https://github.com/codejamninja/reactant/packages/build",
  "engines": {
    "node": ">= 6.0.0"
  },
  "eslintIgnore": [
    "lib",
    "bin"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/[^_]*.js"
    ],
    "verbose": true
  },
  "gitHead": "1d864d8799d8dbb452ff1fd0600afe676a5c0692"
}
