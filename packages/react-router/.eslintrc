{
  "extends": ["jam"],
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "no-empty": "off",
    "prefer-rest-params": "off",
    "import/no-unresolved": [
      "error",
      {
        "ignore": [
          "^~",
          "^./lib",
          "^./reactRouter",
          "^react-router-",
          "^connected-react-router"
        ]
      }
    ]
  }
}
