{
  "extends": ["jam"],
  "globals": {
    "__webpack_hash__": true,
    "document": true,
    "window": true
  },
  "rules": {
    "camelcase": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-empty": "off",
    "prefer-rest-params": "off",
    "react/no-multi-comp": "off",
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["^~", "^@reactant/web-isomorphic"]
      }
    ]
  }
}
