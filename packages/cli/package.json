{
  "name": "@reactant/cli",
  "version": "0.0.70",
  "description": "build tool for reactant",
  "homepage": "https://reactant.codejam.ninja",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "action.js",
    "bin",
    "config",
    "lib",
    "webpack"
  ],
  "scripts": {
    "build": "npm run test && babel src --ignore bin.js -d lib",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "format": "prettier --write ./src/**/*.js",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run build",
    "start": "babel-node src/reactant",
    "test": "npm run lint && jest --forceExit",
    "test:reset": "jest --clearCache && npm run test"
  },
  "bin": {
    "reactant": "bin/reactant.js"
  },
  "dependencies": {
    "@babel/cli": "~7.2.0",
    "@babel/core": "~7.2.2",
    "@babel/polyfill": "~7.0.0",
    "@babel/preset-env": "~7.2.0",
    "@babel/preset-react": "~7.0.0",
    "@ecosystem/config": "~0.2.8",
    "@ecosystem/module-loader": "~0.2.9",
    "@reactant/core": "^0.0.70",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~10.0.1",
    "babel-loader": "~8.0.4",
    "babel-preset-everything": "~0.2.2",
    "bluebird": "~3.5.3",
    "circular-json": "~0.5.9",
    "commander": "~2.19.0",
    "css-loader": "~1.0.1",
    "deasync": "~0.1.14",
    "detect-port": "~1.3.0",
    "err": "~2.1.10",
    "export-es6": "~0.1.0",
    "file-loader": "~2.0.0",
    "flow-bin": "~0.89.0",
    "fs-extra": "~7.0.1",
    "html-loader": "~0.5.5",
    "js-info": "~0.3.0",
    "linked-deps": "~0.1.0",
    "lodash": "~4.17.11",
    "markdown-loader": "~4.0.0",
    "merge-configuration": "~0.4.1",
    "node-ipc": "~9.1.1",
    "ora": "~3.0.0",
    "pkg-dir": "~3.0.0",
    "rc-config": "~0.1.0",
    "trailduck": "~0.2.2",
    "url-loader": "~1.1.2",
    "webpack": "4.28.2"
  },
  "devDependencies": {
    "eslint": "~5.10.0",
    "eslint-config-jam": "~0.1.6",
    "eslint-plugin-jest": "~22.1.2",
    "eslint-plugin-jsx-a11y": "~6.1.2",
    "eslint-plugin-react": "~7.11.1",
    "jest": "~23.6.0",
    "prettier": "~1.15.3"
  },
  "license": "MIT",
  "repository": "https://github.com/codejamninja/reactant/packages/build",
  "engines": {
    "node": ">= 6.0.0"
  },
  "eslintIgnore": [
    "lib",
    "bin"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/[^_]*.js"
    ],
    "verbose": true
  },
  "gitHead": "a4a34397acbc3b96495b2111153c9684c27eb860"
}
