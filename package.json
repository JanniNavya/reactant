{
  "name": "reaction",
  "version": "0.0.1",
  "description": "A react framework",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "node node_modules/parcel-bundler/bin/cli build src/core/client.js",
    "build:server": "node node_modules/cross-env/dist/bin/cross-env BABEL_ENV=node node node_modules/babel-cli/bin/babel src -d dist/server",
    "start": "npm run build && node dist/server/core/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamrizzi/reaction.git"
  },
  "keywords": [
    "react",
    "framework",
    "isomorphic",
    "redux"
  ],
  "author": "Jam Risser <jam@jamrizzi.com> (https://jam.jamrizzi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamrizzi/reaction/issues"
  },
  "homepage": "https://github.com/jamrizzi/reaction#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "parcel-bundler": "^1.4.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
