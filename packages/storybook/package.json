{
  "name": "@reactant/storybook",
  "version": "0.0.14",
  "description": "storybook tooling for reactant",
  "homepage": "https://reactant.codejam.ninja",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run test && babel src --ignore bin.js -d lib",
    "clean": "rimraf lib .cache",
    "format": "prettier --write ./src/**/*.js",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "start": "babel-node src/reactant",
    "test": "npm run lint && jest --forceExit",
    "test:reset": "jest --clearCache && npm run test"
  },
  "bin": {
    "reactant": "bin/reactant.js"
  },
  "dependencies": {
    "@reactant/base": "^0.0.14",
    "@storybook/addon-actions": "^4.0.0-alpha.23",
    "@storybook/addon-links": "^4.0.0-alpha.23",
    "@storybook/addons": "^4.0.0-alpha.23",
    "@storybook/react": "^4.0.0-alpha.23",
    "@storybook/react-native": "^4.0.0-alpha.23",
    "babel-polyfill": "^6.26.0",
    "circular-json": "^0.5.7",
    "lodash": "^4.17.11",
    "native-base": "^2.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-native": "^0.55.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.5.0",
    "eslint-config-jam": "0.1.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "repository": "https://github.com/codejamninja/reactant/packages/storybook",
  "engines": {
    "node": ">= 6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/[^_]*.js"
    ],
    "verbose": true
  },
  "gitHead": "3c667c1477ab22cfad2d5486ab2e8db3077864ab"
}
